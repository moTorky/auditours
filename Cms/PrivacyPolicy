<script type="text/javascript">
    function getdynamicCountry() {
        let countryval = $("#visaDestinationCountry").val();
        let countrytype = "CountryOfResidence";
        $("#visaResidenceCountry").html('');
        if (0 != null && '0' == '0') {
            var selectopt = new Option("Select Residence", "Select Residence");
            $('#visaResidenceCountry').append(selectopt);
        }
        $.ajax({
            url: "/Visa/Home/VisaDynamicCountry",
            data: { Countrycode: countryval, dynamicCountryType: countrytype },
            type: "POST",
            dataType: "json",
            success: function (data) {
                for (var i = 0; i < data.length; i++) {
                    if ('' != null && '' != "") {
                        if ('' == data[i].CountryCode) {
                            var opt = new Option(data[i].CountryName, data[i].CountryCode, true, true);
                        }
                        else {
                            var opt = new Option(data[i].CountryName, data[i].CountryCode);
                        }
                    }
                    else {
                        var opt = new Option(data[i].CountryName, data[i].CountryCode);
                    }
                    $('#visaResidenceCountry').append(opt);
                }
            }
        });
    }

    function getdynamicCountryNationality() {
        let countryval = $("#visaDestinationCountry").val();
        let countrytype = "Nationality";
        $("#ddlCountryOfNationality").html('');
        if (0 != null && '0' == '0') {
            var selectopt = new Option("Select Nationality", "Select Nationality");
            $('#ddlCountryOfNationality').append(selectopt);
        }

        $.ajax({
            url: "/Visa/Home/VisaDynamicCountry",
            data: { Countrycode: countryval, dynamicCountryType: countrytype },
            type: "POST",
            dataType: "json",
            success: function (data) {
                for (var i = 0; i < data.length; i++) {
                    if ('' != null && '' != "") {
                        if ('' == data[i].CountryCode) {
                            var opt = new Option(data[i].CountryName, data[i].CountryCode, true, true);
                        }
                        else {
                            var opt = new Option(data[i].CountryName, data[i].CountryCode);
                        }
                    }
                    else {
                        var opt = new Option(data[i].CountryName, data[i].CountryCode);
                    }
                    $('#ddlCountryOfNationality').append(opt);
                }
            }
        });
    }

    $(document).ready(function () {
        $(".couponcodelist").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: '/Visa/Home/GetCouponList',
                    data: "{'term': '" + request.term + "'}",
                    dataType: "json",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        response($.map(data, function (item) {
                            return {
                                value: item.Text,
                                UserId: item.Value,
                            };
                        }));
                    },
                    error: function (response) {
                        $(".couponcodelist").val("");
                    },
                    failure: function (response) {
                        $(".couponcodelist").val(response.value);
                    }
                });
            },
            select: function (e, i) {
                $(".couponcodelist").val(i.item.UserId);
                return false;
            },
        }).autocomplete("widget").addClass("couponcodeautomatic");

        $("#visaDestinationCountry").change(function () {
            getdynamicCountry()
            getdynamicCountryNationality()
        });

        $("#ddlCountryOfNationality").change(function () {
            var destination = $("#visaDestinationCountry").val();
            var Nationality = $("#ddlCountryOfNationality").val();
            $("#ddlvisatype").html('');
            $("#childage").html('');
            $("#ddlvisatype").append($('<option></option>').val("-").html("-"));
            $.ajax({
                url: '/Visa/Home/GetVisaTypeList',
                data: { destinationCode: destination, NationalityCode: Nationality },
                dataType: "json",
                traditional: true,
                type: "POST",
                success: function (data) {
                    $.each(data, function (id, option) {
                        $("#ddlvisatype").append($('<option></option>').val(option.VisaTypeId).html(option.Description));
                        if (option.Visa_Child_Upper_Age != null && option.Visa_Child_Upper_Age > 0) {
                            $("#childage").html("(Up to " + option.Visa_Child_Upper_Age + ")");
                        }
                    });
                    if ('' != null && '' != 0) {
                        $("#ddlvisatype").val('')
                        $("#ddlvisatype").trigger("change");
                    }

                },
                failure: function () {
                    $("#ddlvisatype").val().html('');
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    $("#ddlvisatype").html('');
                }
            });
        });


        $("#ddlvisatype").change(function () {
            debugger;
            var visatype = $("#ddlvisatype").val();
            var destination = $("#visaDestinationCountry").val();
            $.ajax({
                url: '/Visa/Home/GetVisaTypeIdList',
                data: { "term": visatype, "destination": destination },
                dataType: "json",
                type: "POST",
                success: function (data) {
                    $.each(data, function (id, option) {
                        $("#VisaTypeId").val(option.VisaTypeId);
                        $("#VisaDepartureDate").datepicker("option", "maxDate", option.MaxDays);
                    });
                },
                failure: function () {
                    $("#ddlvisatype").val().html('');
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    $("#ddlvisatype").html('');
                }

            });
        });

        $("#VisaDepartureDate").datepicker("option", "minDate", "&#x2B;0d");
    });

    function SearchVisa() {
        if (($("#ddlvisatype").val() != 'Visatype' && $("#ddlvisatype").val() != "") && $('#visaResidenceCountry').val() != "" && $('#visaDestinationCountry').val() != "") {
            $("#loading-panel").parent().addClass("loading-panel_dialog_box");
            $("#loading-panel").dialog("open");
            $("#loader-page").show();
            if ('0' == '1') {
                grecaptcha.ready(function () {
                    grecaptcha.execute('', { action: 'VisaSearch_CMS' }).then(function (token) {
                        $("#GoogleCaptchaAction").val('VisaSearch_CMS');
                        $("#GoogleCaptchaToken").val(token);
                        var formData = $("#VisasearchForm").serialize();
                        $.post("/Visa/Home/Search", formData,
                            function (data) {
                                if (data == 'Success') {
                                    window.location = "/Visa/Home/Travellers";
                                } else {
                                    CaptchaSettings();
                                    RefreshCaptcha();
                                    $("#loading-panel").dialog("close");
                                    $("#loader-page").hide();
                                    $("#dvError").show();
                                    $("#dvError").html(data);
                                }
                            });
                    });
                });
            }
            else {
                var formData = $("#VisasearchForm").serialize();
                $.post("/Visa/Home/Search", formData,
                    function (data) {
                        if (data == 'Success') {
                            window.location = "/Visa/Home/Travellers";
                        } else {
                            CaptchaSettings();
                            RefreshCaptcha();
                            $("#loading-panel").dialog("close");
                            $("#loader-page").hide();
                            $("#dvError").show();
                            $("#dvError").html(data);
                        }
                    });
            }
        } else {
            setErrorToolTip('');
            if ($("#ddlvisatype").val() == 'Visatype' || $("#ddlvisatype").val() == "") {
                $("#ddlvisatype").addClass('error');

            }
            if ($("#visaResidenceCountry").val() == "") {
                $("#visaResidenceCountry").addClass('error');

            }
            if ($('#visaDestinationCountry').val() == "") {
                $('#visaDestinationCountry').addClass('error');

            }
        }

    }

</script>




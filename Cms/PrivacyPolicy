                                CaptchaSettings();
                                RefreshCaptcha();
                                $("#loading-panel").dialog("close");
                                $("#loader-page").hide();
                                $("#dvError").show();
                                $("#dvError").html(data);
                            }
                        });
                    });
                });
            }
            else {
                var formData = $("#searchForm").serialize();
                //console.log(formData);
                CaptchaCount = CaptchaCount + 1;
                $.post("/Flight/Home/Search", formData, function (data) {

                    if (data == 'Success') {
